      <div class="nice-form-group">
        <label>Email</label>
        <input type="email" placeholder="Your email" value="" />
      </div>

      <div class="nice-form-group">
        <label>Address</label>
        <input type="tel" placeholder="Address" value="" />
      </div>

      <details>
        <summary>

        </summary>
        <script src="https://gist.github.com/nielsVoogt/e25c9c8f2b8456bbd1239b775d21333f.js"></script>
      </details>
